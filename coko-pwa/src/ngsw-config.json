{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls" : [
          "https://font.googleapis.com/**"
        ]

      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups" : [
    {
      "name": "",
      "urls": ["http://51.38.236.246:3000/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 5000,
        "maxAge":"24h",
        "timeout": "5s"
      }
    }
  ]
}
